#!/usr/bin/env bash

#
# HyperNova server management framework
#
# Agent launch bootstrapper
#
# Copyright (c) 2012 TDM Ltd
#                    Luke Carrier <luke.carrier@tdm.info>
#

PYTHON='python3.2'

[ -z "$BINDIR"  ] && BINDIR="/usr/local/hypernova/bin"
[ -z "$CONFDIR" ] && CONFDIR="/usr/local/hypernova/etc/hypernova"
[ -z "$HNDIR"   ] && HNDIR="/usr/local/hypernova/lib/python3.2/site-packages/HyperNova-0.1.0-py3.2.egg/hypernova"

"$BINDIR/$PYTHON" "$HNDIR/client/__init__.py" $@
